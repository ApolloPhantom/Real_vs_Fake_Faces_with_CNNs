{
    "devCommand": "gunicorn -w 4 -b 0.0.0.0:8000 app:app",
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.11" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
}